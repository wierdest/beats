{
  "expo": {
    "name": "beetz",
    "slug": "beetz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "splash": {
      "backgroundColor": "#2d003d",
      "image": "./assets/images/splash.png"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundImage": "./assets/images/monochrome-image.png"
      },
      "package": "com.wierdest.beetz",
      "version": "1.0.0",
      "versionCode": 12
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f20c061c-9fe5-4844-9410-8e944c8a5ac1"
      }
    }
  }
}
